{
  "rules": {
    "quotes": [2, "single"],
    "comma-dangle": [2, "only-multiline"],
    "semi": [2, "always"],
    "func-names": [0],
    "vars-on-top": [0],
    "no-console": [0],
    "indent": [2, 2, {"SwitchCase": 1}],
    "max-len": [0],
    "no-param-reassign": [2, {"props": false} ]
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "extends": "airbnb"
}
